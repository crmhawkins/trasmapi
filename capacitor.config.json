{
  "appId": "com.hawkins.trasmapi",
  "appName": "transmapi",
  "webDir": "dist",
  "bundledWebRuntime": false,
  "android": {
    "webContentsDebuggingEnabled": true,
    "allowMixedContent": true
  },
  "server": {
    "cleartext": true
  },
  "hooks": {
    "postsync": "node scripts/fixAndroidJavaVersion.js"
  },
  "plugins": {
    "CustomURLScheme": {
      "scheme": "trasmapiapp",
      "iosScheme": "trasmapiapp",
      "androidScheme": "trasmapiapp"
    }
  }
}
